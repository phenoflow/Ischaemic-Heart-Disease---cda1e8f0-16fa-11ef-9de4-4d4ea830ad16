      potentialCases:
        id: potentialCases
        source: ischaemic-heart-disease-angina---primary/output
  acute-ischaemic-heart-disease---primary:
    run: acute-ischaemic-heart-disease---primary.cwl
    out:
    - output
    in:
      inputModule:
        id: inputModule
        source: inputModule5
      potentialCases:
        id: potentialCases
        source: ischemic-ischaemic-heart-disease---primary/output
  output-cases:
    run: output-cases.cwl
    out:
    - output
    in:
      inputModule:
        id: inputModule
        source: inputModule6
      potentialCases:
        id: potentialCases
        source: acute-ischaemic-heart-disease---primary/output
