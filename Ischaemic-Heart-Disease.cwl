  ischaemic-heart-disease-infarction---primary:
    run: ischaemic-heart-disease-infarction---primary.cwl
    out:
    - output
    in:
      inputModule:
        id: inputModule
        source: inputModule2
      potentialCases:
        id: potentialCases
